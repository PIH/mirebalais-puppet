RewriteEngine on
RewriteRule ^index.html$ openmrs


# don't cache index.html files, specifically so that OWAs won't be cached
<Files index.html>
FileETag None
Header unset ETag
Header set Cache-Control "max-age=0, no-cache, no-store, must-revalidate"
Header set Pragma "no-cache"
Header set Expires "Wed, 11 Jan 1984 05:00:00 GMT"
</Files>
